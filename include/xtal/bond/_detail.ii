template <auto f, class W>
XTAL_DEF_(inline)
XTAL_LET apply_to(W &w, bool const n_seq=false)
noexcept -> void
{
	w = f(w);
}
template <auto f, class Ws> requires iterable_q<Ws>
XTAL_DEF_(inline)
XTAL_LET apply_to(Ws &ws, bool const n_seq=false)
noexcept -> void
{
	move_to<f>(ws.begin(), ws, n_seq);
}


