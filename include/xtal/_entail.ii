template <class T>
struct aligned
{
	class type {alignas(alignof(T)) ::std::byte data[sizeof(T)];};

	XTAL_DEF_(return,inline)
	XTAL_SET size()
	XTAL_0EX -> size_type
	{
		return sizeof(type);
	}

};

